package com.encore.vo;

public abstract class Book {
	private String isbn;
	private String title;
	private String author;
	private String pulisher;
	private double price;
	private String desc;
	private String kind;
	public Book(String isbn, String title, String author, String pulisher, double price, String desc) {
		this.isbn = isbn;
		this.title = title;
		this.author = author;
		this.pulisher = pulisher;
		this.price = price;
		this.desc = desc;
	}
	
	public abstract String setKind(String name);

	@Override
	public String toString() {
		return "Book [isbn=" + isbn + ", title=" + title + ", author=" + author + ", pulisher=" + pulisher + ", price="
				+ price + ", desc=" + desc + ", kind=" + kind + "]";
	}
	
	
	
}
