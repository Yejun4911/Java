package self.bank;

public class Address {
	private int zipCode;
	private String region;
	private String city;
	
	public Address(int zipCode, String region, String city) {
		this.setZipCode(zipCode);
		this.setRegion(region);
		this.setCity(city);
	}

	public String getAddressInfo() {
		return zipCode+","+region+","+city;
	}
	
}
